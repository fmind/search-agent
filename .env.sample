# The public URL of the agent card, which is a JSON file describing the agent.
# AGENT_CARD=https://your-agent-url.run.app/a2a/search_agent/.well-known/agent-card.json
AGENT_CARD=...
# The numerical ID of your Agent Engine instance found on Google Cloud.
# AGENT_ENGINE_ID=1234567890123456789
AGENT_ENGINE_ID=...
# The service account email with permissions to invoke the agent.
# AGENT_RUN_SA=your-service-account@your-project-id.iam.gserviceaccount.com
AGENT_RUN_SA=...
# The base URL where your agent is hosted.
# AGENT_RUN_URL=https://your-agent-url.run.app
AGENT_RUN_URL=...
# The Google Cloud region where your resources are located.
GOOGLE_CLOUD_LOCATION=us-central1
# Your Google Cloud project ID.
# GOOGLE_CLOUD_PROJECT=your-gcp-project-id
GOOGLE_CLOUD_PROJECT=...
# Set to true to use Vertex AI, false otherwise.
# GOOGLE_GENAI_USE_VERTEXAI=true
GOOGLE_GENAI_USE_VERTEXAI=true
# The Google Cloud Storage bucket for staging agent engine artifacts.
# STAGING_BUCKET=gs://your-staging-bucket-name
STAGING_BUCKET=...
